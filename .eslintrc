{
    "extends": "airbnb-base",
    "env": {
        "node": true,
        "mocha": true
    },
    "globals": {
        "L": true,
        "rootDir": true,
        "getDir": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "max-len": 0,
        "no-nested-ternary": 0,
        "comma-dangle": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "consistent-return": 0,
        "valid-typeof": 0,
        "no-multi-assign": 0,
        "no-param-reassign": 0,
        "no-underscore-dangle": 0,
        "function-paren-newline": [
            "error",
            "multiline"
        ],
        "eqeqeq": 0,
        "func-names": 0,
        "no-restricted-syntax": [
            "error",
            "ForInStatement",
            "LabeledStatement",
            "WithStatement"
        ],
        "no-console": 0,
        "no-return-assign": 0
    }
}